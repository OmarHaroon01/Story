<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="641.0" prefWidth="967.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="App.WallController">
   <children>
      <HBox prefHeight="641.0" prefWidth="967.0">
         <children>
            <VBox prefHeight="641.0" prefWidth="215.0" style="-fx-background-color: #E37BF1;">
               <children>
                  <ImageView fx:id="logo" fitHeight="214.0" fitWidth="266.0" pickOnBounds="true" />
                  <Label fx:id="myProfileLabel" onMouseClicked="#myProfileLabelClicked" prefHeight="46.0" prefWidth="144.0" text="My Profile" textFill="WHITE">
                     <font>
                        <Font size="28.0" />
                     </font>
                     <VBox.margin>
                        <Insets left="30.0" />
                     </VBox.margin>
                     <padding>
                        <Insets left="10.0" />
                     </padding>
                  </Label>
                  <Label fx:id="myUploadsLabel" onMouseClicked="#myUploadsLabelClicked" prefHeight="46.0" prefWidth="163.0" style="-fx-background-radius: 15px;" text="My Uploads" textFill="WHITE">
                     <font>
                        <Font size="28.0" />
                     </font>
                     <padding>
                        <Insets left="10.0" />
                     </padding>
                     <VBox.margin>
                        <Insets left="30.0" top="30.0" />
                     </VBox.margin>
                  </Label>
                  <Label prefHeight="46.0" prefWidth="108.0" style="-fx-background-radius: 15px; -fx-background-color: #d954eb;" text="Stories" textFill="WHITE">
                     <font>
                        <Font size="28.0" />
                     </font>
                     <padding>
                        <Insets left="10.0" />
                     </padding>
                     <VBox.margin>
                        <Insets left="30.0" top="30.0" />
                     </VBox.margin>
                  </Label>
                  <Button fx:id="logoutButton" mnemonicParsing="false" onMouseClicked="#logoutButtonClicked" prefHeight="33.0" prefWidth="98.0" style="-fx-background-color: C0DAF3; -fx-border-color: #d400ff; -fx-background-radius: 15px; -fx-border-radius: 15px;" text="LOGOUT" textFill="#d400ff">
                     <VBox.margin>
                        <Insets left="50.0" top="50.0" />
                     </VBox.margin>
                  </Button>
                  <Label text="Created by Dibya Jyoti Mandal" textFill="WHITE">
                     <font>
                        <Font size="13.0" />
                     </font>
                     <VBox.margin>
                        <Insets left="20.0" top="115.0" />
                     </VBox.margin>
                  </Label>
               </children>
            </VBox>
            <ScrollPane prefHeight="641.0" prefWidth="752.0" style="-fx-background: C0DAF3;">
               <content>
                  <VBox fx:id="vBox" prefHeight="200.0" prefWidth="100.0" />
               </content>
               <HBox.margin>
                  <Insets />
               </HBox.margin>
               <padding>
                  <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
               </padding>
            </ScrollPane>
         </children>
      </HBox>
   </children>
</AnchorPane>
